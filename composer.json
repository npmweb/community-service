{
  "name": "npmweb/community-service",
  "description": "Community Service",
  "repositories": [
    { "type": "vcs", "url": "git@github.com:npmweb/admin-theme-laravel-layout" },
    { "type": "vcs", "url": "git@github.com:npmweb/cdn-helper" },
    { "type": "vcs", "url": "git@github.com:npmweb/client-validation-generator" },
    { "type": "vcs", "url": "git@github.com:npmweb/laravel-base" },
    { "type": "vcs", "url": "git@github.com:npmweb/laravel-forms" },
    { "type": "vcs", "url": "git@github.com:npmweb/laravel-login" },
    { "type": "vcs", "url": "git@github.com:npmweb/laravel-test" },
    { "type": "vcs", "url": "git@github.com:npmweb/laravel-validator-custom-rules" },
    { "type": "vcs", "url": "git@github.com:npmweb/multilevel-organizations" },
    { "type": "vcs", "url": "git@github.com:npmweb/payment" },
    { "type": "vcs", "url": "git@github.com:npmweb/php-helpers" },
    { "type": "vcs", "url": "git@github.com:npmweb/php-env-loader" },
    { "type": "vcs", "url": "git@github.com:npmweb/reference" },
    { "type": "vcs", "url": "git@github.com:npmweb/service-opportunities" },
    { "type": "vcs", "url": "git@github.com:npmweb/upload-handler" }
  ],
  "require": {
    "graham-campbell/flysystem": "~1.0",
    "laravel/framework": "4.2.*",
    "npmweb/laravel-login": "~1.0",
    "npmweb/payment": "dev-master@dev",
    "npmweb/php-env-loader": "dev-master@dev",
    "npmweb/service-opportunities": "dev-master@dev"
  },
  "require-dev": {
    "doctrine/dbal": "v2.4.2",
    "fzaninotto/faker": "1.4.*",
    "mockery/mockery": "0.9.*",
    "npmweb/laravel-test": "~1.0",
    "phpunit/phpunit": "4.2.*",
    "raveren/kint": "0.9.*",
    "symfony/yaml": "2.1.*"
  },
  "autoload": {
    "classmap": [
      "app/database/migrations",
      "app/database/seeds"
    ],
    "psr-4": {
      "NpmWeb\\CommunityService\\": "app/src/NpmWeb/CommunityService",
      "NpmWeb\\Tests\\": "app/tests/NpmWeb/Tests"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev"
}
